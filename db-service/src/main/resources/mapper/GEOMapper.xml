<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.cetasas.db.mapper.GEOMapper">
    <select id="getGeo" resultType="java.lang.String">
        select `geo` from `geo` where `nation` = #{nation};
    </select>
</mapper>