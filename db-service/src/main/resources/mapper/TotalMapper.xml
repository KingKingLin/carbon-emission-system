<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.cetasas.db.mapper.TotalMapper">
    <insert id="insert">
        INSERT INTO `${tableName}` (`e_code`, `value`)
        VALUES (#{total.eCode}, #{total.value});
    </insert>

    <resultMap id="GetAllProvinceMap" type="cn.cetasas.db.pojo.Total">
        <id column="e_code" jdbcType="CHAR" property="eCode" />
    </resultMap>

    <select id="getDataByYear" resultMap="GetAllProvinceMap">
        SELECT `total`.`e_code` AS `e_code`, `e_name` AS `zb`, `value`
        FROM `${tableName}` AS `total`, `e_code`
        WHERE `total`.`e_code` = `e_code`.`e_code`;
    </select>
</mapper>