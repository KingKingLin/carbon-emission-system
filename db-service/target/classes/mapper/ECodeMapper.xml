<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.cetasas.db.mapper.ECodeMapper">
    <insert id="insert" parameterType="cn.cetasas.db.pojo.ECode">
        INSERT INTO `e_code` (`e_code`, `e_name`)
